package test;

public class Pet {
	private String name;
	private String sound;
	private int age;
	private char sex;
	
	public Pet(String name, String sound, int age, char sex) {
		this.name = name;
		this.sound = sound;
		this.age = age;
		this.sex = sex;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSound() {
		return sound;
	}

	public void setSound(String sound) {
		this.sound = sound;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public char getSex() {
		return sex;
	}

	public void setSex(char sex) {
		this.sex = sex;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
